   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "data = np.random.binomial(n=10, p=0.5, size=1000)\n",
    "mean = np.mean(data)\n",
    "\n",
    "print(\"Mean:\", mean)\n",
    "\n",
    "print(\"Matches binomial expectation (n × p = 5).\")\n",
    "\n",
    "plt.hist(data, bins=range(12), density=True);"
   ]
  }
